.voted-thumb-box{
  width: 440rpx;height: 440rpx;
  position: absolute;left: 23%;top: 160px;
}
.thumb2{
  width: 360rpx;height: 360rpx;
  position: absolute;left: 6%;top:-2%;
  animation: thumb2 1s linear;opacity: 0;
  z-index: 15;
  animation-delay: 0.3s;
}
.vote-circle{
  width: 360rpx;height: 360rpx;
  position: absolute;left: 5%;top:-4%;
  animation: circle 1s linear;opacity: 0;

}
@keyframes thumb2{
  15% {
    transform: scale(0.4);
    opacity: 1;
  }
  30% {
    transform: scale(0.8);
    opacity: 0.5;
  }
  100%{
    transform: scale(1);
    opacity: 0;
  }
}
@keyframes circle {
  15% {
    transform: rotate(120deg);
    opacity: 1;
  }
  30% {
    transform: rotate(240deg);
    opacity: 0.5;
  }
  100%{
    transform: rotate(360deg);
    opacity: 0;
  }
}

.bt-img{
  /* position: absolute;z-index: 99; */
  width: 160rpx;
  height: 80rpx;
  margin: 0 -32rpx 24rpx 0;
  animation: voteAni 2s;
}
@keyframes voteAni {
  0%,100% {
    opacity:1;
    transform:scale(1.5)
  }
  50% {
    opacity:.5;
    transform:scale(0.6)
  }
}
.show-number{
  display: block;width: 100%;text-align: center;
  animation: tada .5s .2s ease both;
}
@keyframes tada {
  0%{transform:scale(1);}
  10%,20%{transform:scale(0.6) rotate(-3deg);}
  30%,50%,70%,90%{transform:scale(1) rotate(3deg);}
  40%,60%,80%{transform:scale(1.2) rotate(-3deg);}
  100%{transform:scale(1) rotate(0);}
}
