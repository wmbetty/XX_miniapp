
page{background: #F5F6F8;position: relative;}
.card_wrap {
  background: #f2f2f6;
  position: relative;
  width: 100%;
  height: 100%;
}
.card_wrap_overhide{
  height: 550px;overflow-y: hidden;
  animation: showdown 1s;
}
@keyframes showdown {
  0%{
    opacity: 0.3;
  }
  50%{
    opacity: 0.6;
  }
  100%{
    opacity: 1;
  }
}
.card_wrap .card_item {
  position: absolute;
  width: 92%;background: #fff;
  transform: translateY(34px) translateX(3.4%);
  box-shadow: 0px 2px 6px #eee;
  overflow: hidden;border-radius: 14rpx;
  padding:0 8rpx 40rpx;
  /* height: 920rpx; */
}
.main-head{
  display: flex;justify-content: space-between;padding: 60rpx 40rpx 0;
}
.left-item{width: 80%;display: flex;}
.avatar-box{
  width: 72rpx;height: 72rpx;margin-right: 20rpx;
}
.m-avatar{
  display: block;width: 100%;height: 100%;border-radius: 50%;
  background: #F4F4F8;border: 4rpx solid #efefef;
}
.user-infos{
  padding-top: 12rpx;
  display: flex;
}
.infos-head{font-size: 36rpx;max-width: 240rpx;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.r-share-btn{
  width: 38rpx;height: 38rpx;margin-top: 12rpx;
  background:url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.gender-box{
  width: 30rpx;height: 30rpx;margin: 10rpx 0 0 20rpx;
}
.gender-boy{
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.gender-girl{
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}


.card_wrap .card_item .buy_btn {
  position: absolute;
  bottom: 43px;
  left: 50%;
  margin-left: -65px;
}
.card_wrap .card_item span.change_icon image {
  position: absolute;
  width: 35px;
  height: 11px;
  top: 14px;
  left: 50%;
  margin-left: -17px;
  box-shadow:none;

}
.question-box{
  width: 548rpx;height: 270rpx;margin: 60rpx auto 30rpx;overflow: hidden;
  display: flex;justify-content: center;align-items: center;
  font-size: 48rpx;color: #343434;text-align: center;word-break: break-all;
}
.question-box-hasimg{margin-top: 8rpx;}
.main-totals{
    display: flex;justify-content: center;align-items: center;
    width: 180rpx;border-radius: 30rpx;margin: 0 auto 42rpx;font-size: 28rpx;background: #F4F4F4;color: #C7C7CC;
}
.hot-icon{
    width: 30rpx;height: 30rpx;margin-right: 14rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.choose-box{
  padding: 0 20rpx;display: flex;justify-content: space-between;
}
.choose-item-box{
  width: 46%;height: 320rpx;background: #fff;padding: 0 10rpx;
  box-shadow: 0px 4px 10px rgba(0,0,0,0.1);
  display: flex;justify-content: center;align-items: center;
  font-size: 40rpx;color: #576B95;text-align: center;
  overflow: hidden;position: relative;word-break: break-all;
}
.choose-item-imgbox {height: 376rpx;width: 49%;padding: 0;}
.choose-item-img{
  display: block;width: 100%;height: 100%;
}
.delay-img{
  filter: blur(5px);width: 100%;height: 100%;
  position: absolute;left: 0;top: 0;
}
.hexie-img{
  width: 100%;height: 100%;
  position: absolute;left: 0;top: 0;
}
.vote-mask{
    /*transition: all 0.5s ease-in;*/
    position: absolute;left: 0;top:0;width: 100%;height: 100%;background: rgba(0,0,0, 0.4);color: #fff;
    text-align: center;font-size: 80rpx;font-weight: bold;display: flex;justify-content: center;align-items: center;
    animation: mask-in 1s ease-in;
}
@keyframes mask-in {
  0% {
    opacity: 0.9;
  }
  30% {
    opacity: 0.5;
  }
  60% {
    opacity: 0.7;
  }
  100%{
    opacity: 0.9;
  }

}
.vote-selected{color: #E74C49;}
.user-invite-text{font-size: 28rpx;}
.main-empty-cont{padding-top: 188rpx;}
.up-text-box{
    display: flex;justify-content: center;align-items: center;font-size: 24rpx;padding: 0 20rpx;
    padding-top: 30rpx;color: #C7C7CC;
    position: fixed;left: 38%;
}
.up-icon{
    width: 26rpx;height: 26rpx;
    position: absolute;left: 48%;
    /* background: url('data:image/png;base64,') no-repeat; */
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}
.up-active{
  /* background: url('data:image/png;base64,') no-repeat; */
  background-size: 100% 100%;
  opacity: 0;
}
.no-active{
  animation: arrowAnim 1s infinite;
}

@keyframes arrowAnim {
	0%,100% {
	opacity:1;
	transform:translateY(10px)
}
50% {
	opacity:.5;
	transform:translateY(-1px)
}
}


.share-mask{
    position: fixed;left: 0;top: 0;width: 100%;
    background: rgba(0, 0, 0, 0.5);z-index: 99;
}
.share-btn-container{
    position: absolute;bottom: 20rpx;width: 92%;left: 4%;
}
.slide-up{
  animation: slideup 0.3s linear;
}
@keyframes slideup {
  from {
    transform: translateY(100%);
  }
  to {
    transform: translateY(0);
  }
}
.slide-down{
  animation: slidedown 0.3s linear;
}
@keyframes slidedown {
  from {
    transform: translateY(0);
  }
  to {
    transform: translateY(100%);
  }
}
