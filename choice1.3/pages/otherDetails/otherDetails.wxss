.vate-mask{
    position: absolute;top: 0;left: 0;width: 100%;height: 100%;
    background: rgba(0, 0, 0, 0.4);color: #fff;font-size: 80rpx;
    display: flex;justify-content: center;align-items: center;font-weight: bold;
    animation: mask-in 1s ease-in;
}
@keyframes mask-in {
    0% {
        opacity: 0.9;
    }
    30% {
        opacity: 0.5;
    }
    60% {
        opacity: 0.7;
    }
    100%{
        opacity: 0.9;
    }

}
