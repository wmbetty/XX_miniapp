.title-box{
    border-bottom: 1px solid #f1f1f1;background: #fff;
    width: 100%;height: 500rpx;overflow: hidden;
    display: flex;justify-content: center;align-items: center;
    position: relative;
}
.title-box-sm{height: 270rpx;}
.text-holder{
    text-align:center;color: #888;font-size: 40rpx;
}
.text-areas{
    text-align:center;display:table-cell;vertical-aglin:middle;
    height:540rpx;width:100%;
}
.title-txt-view{
    position: absolute;top:0;left: 0;
    width: 100%;height: 100%;color: #888;
    display: flex;justify-content: center;align-items: center;
}
.title-textarea{
    width: 300rpx;height: 60rpx;padding: 242rpx 0;
    text-align: center;
    font-size: 40rpx;margin: 0 auto;color: #888;
}

.text-holder-white{color: #fff;text-align: center;}
.pub-cont{
    /* background: #fff; */
    display: flex;justify-content: space-between;
}
.pub-cont-box{
    width: 50%;height: 420rpx;overflow: hidden;background: #fff;
    position: relative;
}
.bd-right{border-right: 1px solid #f1f1f1;}
.cont-holder{
    width: 100%;height: 60rpx;font-size:40rpx;text-align: center;margin: 110rpx auto 0;color: #888;
}
.cont-textarea{
    width: 280rpx;height: 200rpx;font-size:40rpx;text-align: center;margin: 70rpx auto 0;
}
.publish-btn{
    background:#E1E1E1;width:100%;height:100rpx;border-radius: 0;
    text-align: center;line-height: 100rpx;color: #fff;
    position: fixed;bottom: 0;left: 0;
}
.publish-btn-active{
    background: #E74C49;color: #fff;
}
.publish-btn::after{border: none;}
.share-mask{
    position: absolute;left: 0;top: 0;width: 100%;
    background: rgba(0, 0, 0, 0.5);z-index: 990;
}
.share-toast{
    width: 690rpx;border-radius: 20rpx;
    position: absolute;top: 50%;left: 50%;z-index: 100;
    margin: -375rpx 0 0 -345rpx;overflow: hidden;
    background: #fff;animation: sharebox 0.5s ease-in;
}
@keyframes sharebox {
    from {
        transform: translateY(100%);
    }
    to {
        transform: translateY(0);
    }
}
.toast-cancle{
    width: 690rpx;border-radius: 20rpx;
    position: absolute;top: 50%;left: 50%;z-index: 100;
    margin: -375rpx 0 0 -345rpx;overflow: hidden;
    background: #fff;animation: canclebox 0.5s ease-out;
}
@keyframes canclebox {
    from {
        transform: translateY(0);
    }
    to {
        transform: translateY(100%);
    }
}
.share-toast-head{
    display: flex;flex-direction: row-reverse;
}
.share-close{
    width: 60rpx;height: 60rpx;margin: 30rpx 30rpx 0 0;
}
.share-infos{
    text-align: center;margin: 50rpx 0 48rpx;color: #343434;font-size: 60rpx;
}
.share-content{
    width: 600rpx;height: 200rpx;color: #888;font-size: 40rpx;text-align: center;
    background: rgba(87, 107, 149, 0.04);margin: 0 auto;padding: 12rpx 20rpx;
    display: flex;justify-content: center;align-items: center;position: relative;
    word-break: break-all;
}
.triangle-down{
    position: absolute;left: 46%;bottom: -30rpx;
    width: 0;height: 0;
    border-left: 28rpx solid transparent;
    border-right: 28rpx solid transparent;
    border-top: 34rpx solid #F3F4F7;
}
.share-btn-box{
    padding: 68rpx 116rpx 60rpx;
    display: flex;justify-content: space-between;
}
.share-btn{
    width: 152rpx;height: 152rpx;
}
.btn-splie-line{
    width: 2px;height: 80rpx;background: #eee;margin-top: 40rpx;
}
.gc-toast{
    width: 240rpx;height: 240rpx;border-radius: 10rpx;
    position: absolute;top: 50%;left: 50%;
    margin: -120rpx 0 0 -120rpx;
    background: rgba(0, 0, 0, 0.5);
}
.toast-cont{
    padding: 60rpx 46rpx 32rpx;display: flex;
}
.toast-info{
    text-align: center;color: #fff;font-size: 26rpx;
}
.smile-icon{
    display: inline-block;
    width: 64rpx;height: 64rpx;margin-right: 24rpx;
    animation: goldRoate 0.6s ease-in;
}
@keyframes goldRoate {
    0%{
        transform: rotateY(360deg);
    }
    100%{
        transform: rotateY(0deg);
    }
}
.score-icon{
    display: inline-block;
    width: 64rpx;height: 68rpx;
}
.share-btn-default::after{border: none;}
.share-btn-default{
    margin: 0;padding: 0;background: none;line-height: none;
}
.imagePathBox{
    width: 100%;
    height: 100%;
