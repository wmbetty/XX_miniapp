}

/*toast start*/
.wxapp-toast-mask{
    opacity: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 888;
}
.wxapp-toast-content-box {
    display: flex;justify-content: center;align-items: center;
    width: 100%;height: 100%;
    position: fixed;z-index: 999;left: 50%;top: 50%;margin-left: -50%;margin-top: -108%;
}
.wxapp-toast-content {
    width: 50%;
    padding: 20rpx;
    background: rgba(0, 0, 0, 0.5);
    border-radius: 10rpx;
}
.wxapp-toast-content-text {
    height: 100%;
    width: 100%;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
}
/*toast end*/
.toast-top{margin-top: -100%;}