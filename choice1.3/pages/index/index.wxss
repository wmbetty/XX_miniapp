.cropper-wrapper{
    position: fixed;left: 0;top: 0;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    justify-content: center;
    height: 100%;width: 100%;
    background-color: #e5e5e5;
    background-color: rgba(0, 0, 0, 0.86);
}

.cropper-buttons{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    justify-content: center;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 40px;
    line-height:40px;
    margin:  5px 0;
}

.cropper-buttons .upload, .cropper-buttons .getCropperImage{
    width: 50%;
