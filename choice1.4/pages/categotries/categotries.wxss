page{background: #F4F4F4;position: relative;padding-bottom: 100rpx;}
.banner-cont{
    width: 750rpx;height: 320rpx;margin: 0 auto;background: #f1f1f1;
}
.banner-cont image{display: block;width: 100%;height: 100%;background: #f1f1f1;}
.ques-tab-head{
    width: 750rpx;height: 64rpx;margin: 0 auto;padding: 16rpx 0;background: #fff;
}
.flex-space-cont{display: flex;justify-content: space-between;}
.tab-head-item{width: 50%;height: 100%;font-size: 36rpx;color: #999;text-align: center;}
.black-line{
    width: 80rpx;height: 10rpx;background: #343434;margin: 8rpx auto 0;
}
.tab-active{color: #343434;}
.ques-tab-content{margin-top: 20rpx;padding-bottom: 30rpx;}
.ques-list-item{
    margin-bottom: 20rpx;background: #fff;padding: 40rpx;
}
.head-left{display: flex;align-items: center;}
.head-right{display: flex;align-items: center;}
.m-avatar{display:block;width:48rpx;height:48rpx;border-radius:50%;
    background:#f4f4f8;border:2rpx solid #efefef;margin-right: 12rpx;}
.u-nickname{font-size:28rpx;max-width:240rpx;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;color: #62559D;margin-right: 20rpx;}
.gender-box{width:24rpx;height:24rpx;}
.gender-boy{background:url('data:image/png;base64,') no-repeat;background-size:100% 100%}
.gender-girl{background:url('data:image/png;base64,') no-repeat;background-size:100% 100%}
.comment-box{display: flex;align-items: center;font-size: 28rpx;color: #888;margin-right: 40rpx;}
.hot-box{display: flex;align-items: center;font-size: 28rpx;color: #888;}
.comm-icon{width: 32rpx;height: 32rpx;margin-right: 12rpx;}
.hot-icon{width:26rpx;height:30rpx;margin-right:14rpx;background:url('data:image/png;base64,') no-repeat;
    background-size:100% 100%;margin-top: -4rpx;}
.list-item-title{width: 100%;min-height: 50rpx;font-size: 36rpx;color: #343434;margin: 30rpx 0;}
.options-container{
    position: relative;width: 330rpx;height: 380rpx;
}
.options-item{
    width: 310rpx;height: 360rpx;font-size: 40rpx;color: #fff;padding: 10rpx;
    display:flex;justify-content:center;align-items:center;word-break: break-all;
}
.options-item-img{width: 100%;height: 100%;}
.green-bg{background: #11B591;}
.purple-bg{background: #62559D;}
.bg-opacity{opacity: .7}
.list-item-botom{margin-top: 20rpx;font-size: 28rpx;color: #343434;padding: 30rpx;background: #E5E7ED;border-radius: 10rpx;}
.botom-title{color: #62559D;margin-bottom: 10rpx;}
.nomore-text{color:#c7c7c7;font-size:24rpx;text-align:center;padding:20rpx 0}
.tab-content-empty{padding-top:80rpx;margin-bottom:140rpx}
.vote-mask{position:absolute;left:0;top:0;width:100%;height:100%;background:rgba(0,0,0,0.4);color:#fff;text-align:center;font-size:80rpx;font-weight:bold;display:flex;justify-content:center;align-items:center;animation:mask-in 1s ease-in}
@keyframes mask-in
{
    0%{opacity:.9}
    30%{opacity:.5}
    60%{opacity:.7}
    100%{opacity:.9}
}
.vote-selected{color:#e74c49}
.show-number{display:block;width:100%;text-align:center;animation:tada .5s .2s ease both}
@keyframes tada{
    0%{transform:scale(1)}
    10%,20%{transform:scale(0.6) rotate(-3deg)}
    30%,50%,70%,90%{transform:scale(1) rotate(3deg)}
    40%,60%,80%{transform:scale(1.2) rotate(-3deg)}
    100%{transform:scale(1) rotate(0)}
}
.delay-img{filter:blur(5px);width:100%;height:100%;position:absolute;left:0;top:0}
.hexie-img{width:100%;height:100%;position:absolute;left:0;top:0}
.voted-thumb-box{width:440rpx;height:440rpx;position:fixed;left:23%;top:160px;}
.thumb2{width:360rpx;height:360rpx;position:absolute;left:6%;top:-2%;animation:thumb2 1s linear;opacity:0;z-index:15;animation-delay:.3s}
.vote-circle{width:360rpx;height:360rpx;position:absolute;left:5%;top:-4%;animation:circle 1s linear;opacity:0}
@keyframes thumb2{
    15%{transform:scale(0.4);opacity:1}
    30%{transform:scale(0.8);opacity:.5}
    100%{transform:scale(1);opacity:0}
}
@keyframes circle{
    15%{transform:rotate(120deg);opacity:1}
    30%{transform:rotate(240deg);opacity:.5}
    100%{transform:rotate(360deg);opacity:0}
}
.fixed-botom-container{
    width: 100%;height: 100rpx;background: #fff;border-top: 1px solid #f1f1f1;
    position: fixed;left: 0;bottom: 0;display: flex;align-items: center;justify-content: space-between;
}
.topic-botom-btn{width: 330rpx;height: 72rpx;}
.left-btn{margin-left: 30rpx;}
.right-btn{margin-right: 30rpx;}
button::after{border: none;}
.share-btn-botom{background: #62559D;border-radius: 0;padding: 0;}
.topic-botom-btn image{display: block;width: 330rpx;height: 72rpx;}
.tab-head-fixed{position: fixed;width: 100%;left: 0;top: 0;background: #fff;z-index: 999}
.list-item-percent{position: relative;padding-bottom: 24rpx}
.percent-item{width: 330rpx;text-align: center;font-size: 52rpx;}
.green-font{color: #11B591}
.purple-font{color: #62559D}
.voter-left-box{width: 52rpx;height: 74rpx;position: absolute;left: 0;top:4rpx;}
.voter-right-box{width: 52rpx;height: 74rpx;position: absolute;right: 0;top:4rpx;}
.voter-img{display: block;width: 52rpx;height: 52rpx;margin-bottom: 10rpx;border-radius: 50%;border: 2rpx solid #f1f1f1;background: #f1f1f1}
.vote-down-arrow{display: block;width: 20rpx;height: 12rpx;margin: 0 auto;}
