page{background: #fff;}
.banner-cont{
    width: 750rpx;height: 320rpx;margin: 0 auto;
}
.banner-cont image{display: block;width: 100%;height: 100%;background: #f1f1f1;}
.flex-space-cont{display: flex;justify-content: space-between;}
.tab-head{padding: 16rpx 0;width: 700rpx;height: 84rpx;}
.tab-head-item{
    width: 33.3%;color: #888;font-size: 36rpx;position: relative;
    display: flex;justify-content: center;align-items: center;
}
.rank-arrow{
    width: 20rpx;height: 24rpx;position: absolute;top: 24rpx;right: 48rpx;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100% 100%;
}
.rank-arrow-up{background: url("data:image/png;base64,") no-repeat;
background-size: 100% 100%;}
.tab-text{
    border-bottom: 10rpx solid #fff;padding-bottom: 8rpx;border-bottom: 10rpx solid #fff;
}
.border-fff{border-bottom: 10rpx solid #fff;}
.tab-black-text{
    color: #343434;border-bottom: 10rpx solid #343434;padding-bottom: 8rpx;
}
.rank-medal{
    width: 46rpx;height: 72rpx;margin-right: 26rpx;text-align: center;
}
.tab-cont{padding: 40rpx 30rpx 20rpx;}
.rank-list-item{display: flex;margin-bottom: 40rpx;}
.ques-user-info{
    display: flex;align-items: center;color: #62559D;font-size: 28rpx;
    position: relative;
}
.user-avatar{
    width: 48rpx;height: 48rpx;border-radius:50%;border:2rpx solid #efefef;
    background: #efefef;margin-right: 12rpx;
}
.ques-cont{
    width: 620rpx;min-height: 56rpx;border-radius: 10rpx;margin: 16rpx auto 0;
    background: #F5F6F8;font-size: 40rpx;color: #343434;
}
.ques-title{padding: 40rpx;}
.one-num{
    color: #E64340;font-size: 52rpx;
}
.two-num{
    color: #E64340;font-size: 44rpx;
}
.three-num{
    color: #E64340;width: 62rpx;font-size: 44rpx;
}
.tab-content-empty{padding-top:80rpx;margin-bottom:140rpx}
.nomore-text{color:#c7c7c7;font-size:24rpx;text-align:center;padding:20rpx 0}
.tab-head-fixed{position: fixed;width: 100%;left: 0;top: 0;background: #fff;z-index: 999}
.my-ques-tag{position: absolute;top: 0;right: 0;width: 34rpx;height: 48rpx;}